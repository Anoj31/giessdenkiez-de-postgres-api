{
    "version": 2,
    "name": "trees-express-now",
    "builds": [{
        "src": "index.js",
        "use": "@now/node"
    }],
    "routes": [{
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
            "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
        },
        "src": "/.*",
        "dest": "/index.js"
    }],
    "env": {
        "user": "@user",
        "database": "@database",
        "password": "@password",
        "port": "@port",
        "host": "@host"
    }
}
